    "ng-block-ui": "^2.1.8",
    "ng2-completer": "3.0.3",
    "ng2-smart-table": "1.5.0",
    "normalize.css": "6.0.0",
    "pace-js": "1.0.2",
    "roboto-fontface": "0.8.0",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.3.0",
